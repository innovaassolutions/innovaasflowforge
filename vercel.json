{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "archetypequiz.leadingwithmeaning.com"
        }
      ],
      "destination": "/flowforge/"
    },
    {
      "source": "/:path((?!flowforge|_next|api).*)",
      "has": [
        {
          "type": "host",
          "value": "archetypequiz.leadingwithmeaning.com"
        }
      ],
      "destination": "/flowforge/:path"
    }
  ]
}
