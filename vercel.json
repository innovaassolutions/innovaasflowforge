{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "^(?!.*\\.vercel\\.app$|.*innovaas\\.co$|localhost.*).*$"
        }
      ],
      "destination": "/flowforge/:path*"
    }
  ]
}
