# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Anthropic Claude API
ANTHROPIC_API_KEY=your_anthropic_api_key

# OpenAI API (for embeddings)
OPENAI_API_KEY=your_openai_api_key

# Resend Email Service
RESEND_API_KEY=your_resend_api_key

# Application URL (for generating access links in emails)
NEXT_PUBLIC_APP_URL=http://localhost:3000

# CRM Supabase (for analytics and lead capture)
# These connect to a separate CRM database for storing landing page analytics
# Use the JWT anon key format, NOT the sb_publishable_ format
NEXT_PUBLIC_CRM_SUPABASE_URL=your_crm_supabase_url
NEXT_PUBLIC_CRM_SUPABASE_ANON_KEY=your_crm_supabase_anon_key_jwt_format
