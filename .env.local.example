# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Anthropic Claude API
ANTHROPIC_API_KEY=your_anthropic_api_key

# OpenAI API (for embeddings)
OPENAI_API_KEY=your_openai_api_key

# Resend Email Service
RESEND_API_KEY=your_resend_api_key
# Optional: Verified domain email for production (e.g., noreply@yourdomain.com)
# If not set, falls back to onboarding@resend.dev (only works for Resend account owner email)
RESEND_FROM_EMAIL=

# Application URL (for generating access links in emails)
NEXT_PUBLIC_APP_URL=http://localhost:3000

# CRM Supabase (for analytics and lead capture)
# These connect to a separate CRM database for storing landing page analytics
# Use the JWT anon key format, NOT the sb_publishable_ format
NEXT_PUBLIC_CRM_SUPABASE_URL=your_crm_supabase_url
NEXT_PUBLIC_CRM_SUPABASE_ANON_KEY=your_crm_supabase_anon_key_jwt_format

# ElevenLabs Voice Integration
# For real-time voice interviews using ElevenLabs Conversational AI
# Get these from: https://elevenlabs.io/app/conversational-ai
ELEVENLABS_API_KEY=your_elevenlabs_api_key
ELEVENLABS_AGENT_ID=your_elevenlabs_agent_id
# Secret for authenticating LLM endpoint requests from ElevenLabs
# Generate a secure random string (e.g., openssl rand -hex 32)
ELEVENLABS_LLM_SECRET=your_secure_random_secret

# Cloudflare for SaaS (Custom Domains)
# For enabling custom subdomains on tenant profiles
# Get these from: https://dash.cloudflare.com (Zone > Overview + API Tokens)
# API Token needs Zone:Edit permission for Custom Hostnames
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token
# Zone ID for innovaas.co (found in Zone Overview > API section)
CLOUDFLARE_ZONE_ID=your_cloudflare_zone_id
# CNAME target for custom hostnames (from Cloudflare for SaaS setup)
# Usually: flowforge.ssl.cloudflare.com or similar
CLOUDFLARE_CNAME_TARGET=your_cloudflare_cname_target
